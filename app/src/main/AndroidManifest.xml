<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tlf.com.tlfdebug">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.BaseActivity" />
        <activity android:name=".ui.BaseBleActivity" />
        <activity android:name=".ui.WeightActivity" />
        <activity android:name=".ui.BpActivity" />
        <activity android:name=".ui.BsActivity" />
        <activity android:name=".ui.OxyActivity" />
        <activity android:name=".ui.TempActivity" />
    </application>

</manifest>